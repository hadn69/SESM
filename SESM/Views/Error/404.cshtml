    <script>
        jQuery(document).ready(function () {
            Metronic.init(); // init metronic core components
            Layout.init(); // init current layout

            var winWidth = $(window).width();
            var winHeight = $(window).height();

            var score;
            var bestScore = 0;
            var gameOver;
            var timerObject;
            var asteroTimer;
            var asteroSpeed;

            var asteroTimerBase = 800;
            var asteroSpeedBase = 5000;

            var currentID;

            var rng;

            var srcAstero = "/assets/admin/pages/media/pages/asteroid.png";
            var srcTeapot = "/assets/admin/pages/media/pages/teapot.png";
            var srcServer = "/assets/admin/pages/media/pages/server.png";

            var seed = "SESMSESM";

            function getParameterByName(name) {
                name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
                var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                    results = regex.exec(location.search);
                return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
            }

            if (getParameterByName("seed") != "") {
                seed = getParameterByName("seed");
            }

            if (getParameterByName("speed") != "") {
                asteroSpeedBase = getParameterByName("speed");
            }

            if (getParameterByName("timer") != "") {
                asteroTimerBase = getParameterByName("timer");
            }

            $("#btnRestartGO").click(function () {
                reset();
                spanwAstero();
            });
            $("#btnRestart").click(function () {
                if (confirm("Are you sure you want to restart ?")) {
                    reset();
                    spanwAstero();
                }
            });
            reset();

            function reset() {
                score = 404;
                $("#scoreSpan").text(score);
                gameOver = false;
                asteroTimer = asteroTimerBase;
                asteroSpeed = asteroSpeedBase;
                currentID = 0;
                $(".gameoverdiv").hide();
                rng = new RNG(seed);
            }


            $(window).resize(function () {
                winWidth = $(window).width();
                winHeight = $(window).height();
            });
            function gameOverCall() {
                if (!gameOver) {
                    gameOver = true;
                    $("#newRecord").hide(0);
                    if (score > bestScore) {
                        bestScore = score;
                        $("#newRecord").show(0);
                    }
                    $("#bestScore").text(bestScore);
                    $("#finalScore").text(score);
                    $(".gameoverdiv").show();
                    rng = new RNG(seed);
                    $("#asteroids").children().each(function () {
                        $(this).remove();
                    });
                    clearTimeout(timerObject);
                }
            }
            function spanwAstero() {
                asteroSpeed -= 10;
                if (asteroSpeed < 300)
                    asteroSpeed = 300;

                var UsedSrc = srcAstero;
                if (score == 418) {
                    UsedSrc = srcTeapot;
                }
                else if (score == 500) {
                    UsedSrc = srcServer;
                }

                $("#asteroids").append('<img id="asteroid' + currentID + '" class="spinningObject' + rng.random(1, 7) + '" src="' + UsedSrc + '"/>');
                $("#asteroids").children().last().css({ width: "+=" + rng.random(-30, 30) + "px", left: "-300px", top: rng.random(-128, winHeight - 128) + "px" })
                    .animate({ left: winWidth + 'px' }, {
                        duration: asteroSpeed + rng.random(-500, 500),
                        easing: "linear",
                        progress: function () {
                            var astroRadius = $("#spinningAstronaute").width() / 600 * 220;
                            var astroX = $("#spinningAstronaute").css("left");
                            astroX = parseInt(astroX.substring(0, astroX.length - 2)) + $("#spinningAstronaute").width() / 2;
                            var astroY = $("#spinningAstronaute").css("top");
                            astroY = parseInt(astroY.substring(0, astroY.length - 2)) + $("#spinningAstronaute").height() / 2;

                            var asteroRadius = $(this).width() / 256 * 75;
                            var asteroX = $(this).css("left");
                            asteroX = parseInt(asteroX.substring(0, asteroX.length - 2)) + $(this).width() / 2;
                            var asteroY = $(this).css("top");
                            asteroY = parseInt(asteroY.substring(0, asteroY.length - 2)) + $(this).height() / 2;

                            if (Math.pow(asteroX - astroX, 2) + Math.pow(astroY - asteroY, 2) <= Math.pow(astroRadius + asteroRadius, 2)) {
                                gameOverCall();
                            }

                        },
                        complete: function () {
                            if (!gameOver) {
                                score++;
                                $("#scoreSpan").text(score);
                            }
                            $(this).remove();
                        }
                    });
                currentID++;
                asteroTimer -= 10;
                if (asteroTimer < 200)
                    asteroTimer = 200;

                var nextCall = asteroTimer - rng.random(-250, 250);
                timerObject = setTimeout(function () {
                    spanwAstero();
                }, nextCall);
            }

            $("body").click(function () {
                $("body").off("click");
                astromouse();
                $("#404UI").hide(1000, function () {
                    $("#GAMEUI").show(1000, function () {
                        spanwAstero();
                    });
                });
            });

            function getAstroPos(astroWidth, astroHeight) {
                var X = event.pageX - astroWidth / 2;
                var Y = event.pageY - astroHeight / 2;

                if (X < 0) {
                    X = 0;
                }
                if (Y < 0) {
                    Y = 0;
                }

                if (X > winWidth - astroWidth) {
                    X = winWidth - astroWidth;
                }
                if (Y > winHeight - astroHeight) {
                    Y = winHeight - astroHeight;
                }

                return { X: X, Y: Y };
            }

            function astromouse() {
                $("#spinningAstronaute").css({ top: $("#spinningAstronaute").offset().top, left: $("#spinningAstronaute").offset().left });

                var astroWidth = $("#spinningAstronaute").width() * 40 / 100;
                var astroHeight = $("#spinningAstronaute").height() * 40 / 100;

                var astroPos = getAstroPos(astroWidth, astroHeight);

                $("#spinningAstronaute").animate({ width: '8%', left: astroPos.X, top: astroPos.Y }, {
                    duration: 1000, queue: false, complete: function () {
                        $("body").mousemove(function (event) {
                            astroWidth = $("#spinningAstronaute").width();
                            astroHeight = $("#spinningAstronaute").height();
                            astroPos = getAstroPos(astroWidth, astroHeight);
                            $("#spinningAstronaute").animate({ left: astroPos.X, top: astroPos.Y }, { duration: 100, queue: false, easing: 'easeOutExpo' });
                        });
                    }
                });
            }
        });
    </script>
